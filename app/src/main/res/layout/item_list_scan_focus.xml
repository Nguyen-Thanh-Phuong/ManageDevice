        android:textSize="16sp"
        android:textStyle="bold"
        folding-cell:layout_constraintStart_toStartOf="@+id/txv_time_shied"
        folding-cell:layout_constraintTop_toBottomOf="@+id/txv_time_shied" />

    <TextView
        android:id="@+id/txv_date_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="KJ19JUN00007178"
        folding-cell:layout_constraintStart_toStartOf="@+id/textView15"
        folding-cell:layout_constraintTop_toBottomOf="@+id/textView15" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@drawable/bgr_edt_blue"
        android:src="@drawable/shield"
        folding-cell:layout_constraintStart_toStartOf="@+id/imageView2"
        folding-cell:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:background="@drawable/bgr_edt_blue"
        android:src="@drawable/pin"
        folding-cell:layout_constraintStart_toStartOf="@+id/imageView3"
        folding-cell:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Thông số :"
        android:textSize="16sp"
        android:textStyle="bold"
        folding-cell:layout_constraintStart_toStartOf="@+id/txvSo_Che_Tao"
        folding-cell:layout_constraintTop_toBottomOf="@+id/txvSo_Che_Tao" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/bgr_edt_blue"
        android:src="@drawable/so_ban_tao"
        folding-cell:layout_constraintStart_toStartOf="parent"
        folding-cell:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>