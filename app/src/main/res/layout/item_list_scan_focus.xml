    <TextView
        android:id="@+id/txv_user_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="KJ19JUN00007178"
        folding-cell:layout_constraintStart_toStartOf="@+id/textView13"
        folding-cell:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="Thời gian B.Hành :"
        android:textSize="16sp"
        android:textStyle="bold"
        folding-cell:layout_constraintStart_toStartOf="@+id/txv_parameter"
