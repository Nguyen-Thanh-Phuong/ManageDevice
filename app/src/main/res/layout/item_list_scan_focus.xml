        folding-cell:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:background="@drawable/bgr_edt_blue"
        android:src="@drawable/user_picture"
        folding-cell:layout_constraintStart_toStartOf="@+id/imageView4"
        folding-cell:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/txv_where_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="KJ19JUN00007178"
        folding-cell:layout_constraintStart_toStartOf="@+id/textView12"
        folding-cell:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/txv_time_shied"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="KJ19JUN00007178"
        folding-cell:layout_constraintStart_toStartOf="@+id/textView9"
        folding-cell:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Ngày Nhập  :"
        android:textSize="16sp"
        android:textStyle="bold"
        folding-cell:layout_constraintStart_toStartOf="@+id/txv_user_input"
        folding-cell:layout_constraintTop_toBottomOf="@+id/txv_user_input" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
