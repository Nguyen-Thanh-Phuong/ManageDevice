<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:folding-cell="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ui.search.Result_Search_QRFragment"
    android:layout_marginBottom="20dp"
    android:layout_margin="10dp"
    android:background="@drawable/bgr_edt_blue"
    android:visibility="gone"
    >

    <TextView
        android:id="@+id/txv_parameter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="KJ19JUN00007178"
        folding-cell:layout_constraintStart_toStartOf="@+id/textView7"
        folding-cell:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Người Nhập  :"
        android:textSize="16sp"
        android:textStyle="bold"
        folding-cell:layout_constraintStart_toStartOf="@+id/txv_where_map"
        folding-cell:layout_constraintTop_toBottomOf="@+id/txv_where_map" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bgr_edt_blue"
